{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nsaquib221122.boltexponativewind",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "jsEngine": "jsc"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "hermesEnabled": true
          }
        }
      ],
      "expo-dev-client",
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.nsaquib221122.boltexponativewind"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c36693e8-fb14-4366-98b2-c68af64a8af2"
      }
    }
  }
}
